{
  "name": "r0073rr0r/laravel-webauthn",
  "description": "Laravel Jetstream and Livewire package for WebAuthn authentication (biometrics, USB security keys, and passkeys)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "r0073rr0r",
      "email": "majstorov@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "laravel/framework": "^12.0",
    "livewire/livewire": "^3.0 || ^4.0",
    "laravel/jetstream": "^5.0",
    "spomky-labs/cbor-php": "^3.1",
    "web-auth/webauthn-framework": "^5.2",
    "ext-openssl": "*"
  },
  "require-dev": {
    "laravel/pint": "^1.25",
    "orchestra/testbench": "^10.6",
    "pestphp/pest": "^3.8",
    "pestphp/pest-plugin-laravel": "^3.2",
    "phpunit/phpunit": "^11.5"
  },
  "autoload": {
    "psr-4": {
      "r0073rr0r\\WebAuthn\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "r0073rr0r\\WebAuthn\\Tests\\": "tests/",
      "App\\": "tests/Support/"
    },
    "files": [
      "tests/Support/overrides.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "r0073rr0r\\WebAuthn\\WebAuthnServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "pestphp/pest-plugin": true
    }
  }
}
